<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	 <ui:style src="Style.css" />
	<g:HTMLPanel>
		<div ui:field='imageDiv' class='{style.imageDiv}'>
			<g:Image ui:field='promptImage' styleName="{style.dialogImage}" url="Theme/question.png"/>
		</div>
		<div ui:field='dialogDiv' class='{style.loginDialogMessageDiv}'>
			<g:Label ui:field='promptMessage'  styleName="{style.loginDialogMessage}"></g:Label>
		</div>
		<div class='{style.spacer}'></div>
		<div ui:field='userNameDiv' class='{style.loginTextBoxes}'>
		  <g:Label ui:field='userNameLable'  styleName="{style.userLbl}">Usere Name: </g:Label>&nbsp;&nbsp;&nbsp;<g:TextBox ui:field='userName'  styleName="{style.inputText}"></g:TextBox><br/>
		  <g:Label ui:field='passwordNameLable'  styleName="{style.userLbl}">Passord :</g:Label>&nbsp;&nbsp;&nbsp;<g:PasswordTextBox ui:field='password'  styleName="{style.inputText}"></g:PasswordTextBox><br/>
		  <g:CheckBox ui:field='checkBox'  styleName="{style.inputText}"> remember the password</g:CheckBox><br/>
		</div>
		<div ui:field='responceDiv' class='{style.responceDiv}'>
			<g:Button styleName="{style.dialogButton}" ui:field="loginButton" text="Login"/>
			<g:Button styleName="{style.dialogButton}" ui:field="cancelButton" text="Cancel"/>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 